                "PYTHONPATH": "${workspaceFolder}/devel/lib/python3/dist-packages:${env:PYTHONPATH}",
                "ROS_PACKAGE_PATH": "${workspaceFolder}/src:${env:ROS_PACKAGE_PATH}",
                "CMAKE_PREFIX_PATH": "${workspaceFolder}/devel:${env:CMAKE_PREFIX_PATH}"
            },
            "args": []
